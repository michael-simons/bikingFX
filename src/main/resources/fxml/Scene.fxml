<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="750" prefWidth="900.0" stylesheets="@../styles/Styles.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ac.simons.bikingFX.FXMLController">
   <children>
      <HBox fx:id="test" alignment="CENTER_LEFT" maxHeight="113.0" minHeight="113.0" prefHeight="113.0" styleClass="biking-pictures-container" />
      <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
        <tabs>
          <Tab text="%tab.bikes">
               <content>
                  <TableView fx:id="viewBikes" editable="true" prefHeight="200.0" prefWidth="200.0">
                    <columns>
                      <TableColumn fx:id="viewBikeName" editable="false" prefWidth="75.0" text="%bike.name" />
                      <TableColumn fx:id="viewBikeBoughtOn" prefWidth="101.0" styleClass="col-right" text="%bike.boughtOn" />
                        <TableColumn fx:id="viewBikeDecommissionedOn" prefWidth="49.0" styleClass="col-right" text="%bike.decommissionedOn" />
                        <TableColumn fx:id="viewBikeMilage" editable="false" prefWidth="300.0" styleClass="col-right" text="%bike.milage" />
                        <TableColumn fx:id="viewBikeColor" prefWidth="60.0" sortable="false" styleClass="col-center" text="%bike.color" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </content></Tab>
          <Tab text="%tab.gallery" />
        </tabs>
      </TabPane>
   </children>
</VBox>
